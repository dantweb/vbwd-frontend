    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "happy-dom": "^12.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
